// Check it the email is valid and if the member has voted already to display
// the voting table
if(<%= @isValid %>){
	if(<%= @voted %>){
		$('.bg-warning').show();
	}else {
		$('.table-responsive').show();
	}
}
else{
	$('.bg-danger').show();
};